<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.gxnu.mapper.StudentMapper">

    <resultMap id="BaseResultMap" type="com.gxnu.pojo.Student">
            <id property="studentId" column="student_id" jdbcType="INTEGER"/>
            <result property="studentNumber" column="student_number" jdbcType="VARCHAR"/>
            <result property="studentPassword" column="student_password" jdbcType="VARCHAR"/>
            <result property="studentEmail" column="student_email" jdbcType="VARCHAR"/>
            <result property="studentName" column="student_name" jdbcType="VARCHAR"/>
            <result property="studentSex" column="student_sex" jdbcType="CHAR"/>
            <result property="studentAcademy" column="student_academy" jdbcType="VARCHAR"/>
            <result property="studentMajor" column="student_major" jdbcType="VARCHAR"/>
            <result property="totalTime" column="total_time" jdbcType="INTEGER"/>
            <result property="totalCost" column="total_cost" jdbcType="INTEGER"/>
            <result property="isDeleted" column="is_deleted" jdbcType="TINYINT"/>
            <result property="version" column="version" jdbcType="INTEGER"/>
    </resultMap>

    <sql id="Base_Column_List">
        student_id,student_number,student_password,
        student_email,student_name,student_sex,
        student_academy,student_major,total_time,
        total_cost,is_deleted,version
    </sql>
</mapper>
